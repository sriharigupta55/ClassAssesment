#include <iostream>

using namespace std;

int main()
{
    int arr[5] = {1,0,2,4,0};
    int k = 0, j = 1;
    for(int i = 0;i<5;i++){
        if(arr[k]==0){
            swap(arr[k],arr[j]);
        }
        k++;
        j++;
        //cout<<k<<j<<'\n';
    }
    for(int i = 0; i < 5; i++){
        cout<<arr[i]<<' ';
    }

    return 0;
}
